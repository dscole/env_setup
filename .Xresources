!! config for urxvt
URxvt*formatOtherKeys: 1
URxvt*modifyOtherKeys: 1
URxvt*vt100.modifyOtherKeys: 1

! This is from arankine, supposedly he can make it work and show all of these
! glyphs with no issues:
!  ⑧ ⓐ ⓒ ⓔ Ⓔe Ⓔh ⓕ Ⓕ ⓗi Ⓚ Ⓢ ⓨ
! ! Emacs.font:              Source Code Pro-10

! ! URxvt*font:              xft:Source Code Pro:size=10
! ! URxvt*letterSpace:       -1
! ! URxvt.scrollstyle:       plain
! ! URxvt.scrollBar_right:   true
! ! URxvt.background:        black
! ! URxvt.foreground:        white
! ! URxvt.secondaryScreen:   1
! ! URxvt.secondaryScroll:   0

! ! ! Fonts {{{
! ! Xft.antialias: true
! ! Xft.hinting:   true
! ! Xft.rgba:      rgb
! ! Xft.hintstyle: hintfull
! ! Xft.dpi:       160
! ! ! }}}

URxvt*utf8: 2
URxvt*vt100.utf8: 2

URxvt*bellIsUrgent: true
